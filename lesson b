Урок В
задача 1

#include <stdio.h>
int main()
{
    int a=1, b;
    scanf("%d", &b);
    while (a <= b) {
    printf("%d ", a);
    printf("%d ", a*a);
    printf("%d \n", a*a*a);
    a++;
    }
     return 0;
}


задача 2
#include <stdio.h>

int main()
{
    int a, b;
    scanf("%d%d", &a, &b);
    while (a <= b) {
    printf("%d  ", a*a);
    a++;
    }
     return 0;
}


задача 3
#include <stdio.h>

int main()
{
    int a, b, sum;

    scanf("%d%d", &a, &b);

    while (a <= b)
    {
        sum += a*a;

        a++;

    }

    printf("%d", sum);

    return 0;
}


задача 4
#include <stdio.h>

int main()
{
    int a, b=99, c=999;

    scanf ("%d", &a);

    if (a > b && a <= c) {
        printf("YES");
    }
    
    else {
        printf("NO");
    }
        
    return 0;
}

задача 5
#include <stdio.h>

int main()
{
    int a, sum=0;

    scanf("%d", &a);

    while (a != 0)
    {

       // printf("%d\n", a%10);

        sum += a%10;

        a /=10;

    //    printf("%d\n\n", sum);

    }

    printf("%d", sum);

    return 0;
}


