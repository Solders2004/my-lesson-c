задача 1 ##########################################################################################################
/* Ввести натуральное число вывести квадраты и кубы всех чисел от 1 до этого числа. Число не превосходит 100. */

#include <stdio.h>
int main()
{
    int a=1, b;
    scanf("%d", &b);
    while (a <= b) {
    printf("%d ", a);
    printf("%d ", a*a);
    printf("%d \n", a*a*a);
    a++;
    }
     return 0;
}


задача 2 ##########################################################################################################
/*Ввести два целых числа a и b (a ≤ b) и вывести квадраты всех чисел от a до b.*/

#include <stdio.h>

int main()
{
    int a, b;
    scanf("%d%d", &a, &b);
    while (a <= b) {
    printf("%d  ", a*a);
    a++;
    }
     return 0;
}


задача 3 ##########################################################################################################
/*Ввести два целых числа a и b (a ≤ b) и вывести сумму квадратов всех чисел от a до b.*/

#include <stdio.h>

int main()
{
    int a, b, sum;

    scanf("%d%d", &a, &b);

    while (a <= b)
    {
        sum += a*a;

        a++;

    }

    printf("%d", sum);

    return 0;
}


задача 4 ##########################################################################################################
/* Ввести целое число и определить, верно ли, что в нём ровно 3 цифры. */

#include <stdio.h>

int main()
{
    int a, b=99, c=999;

    scanf ("%d", &a);

    if (a > b && a <= c) {
        printf("YES");
    }
    
    else {
        printf("NO");
    }
        
    return 0;
}

задача 5 ##########################################################################################################
/* Ввести целое число и найти сумму его цифр. */

#include <stdio.h>

int main()
{
    int a, sum=0;

    scanf("%d", &a);

    while (a != 0)
    {

       // printf("%d\n", a%10);

        sum += a%10;

        a /=10;

    //    printf("%d\n\n", sum);

    }

    printf("%d", sum);

    return 0;
}


задача 6 ##########################################################################################################

/* Ввести целое число и определить, верно ли, что в его записи есть две одинаковые цифры, стоящие рядом.*/

#include <stdio.h>

int main(void)
{
int a;
unsigned char k;
k = 0;

scanf("%d", &a);

while(a)
{
    if((a/10)%10 == a%10)
        k = 1;
    a/=10;  
}
if(k==1)
  printf("YES");   
else
  printf("NO");
 
return 0;
}


задача 8 ##########################################################################################################
#include <stdio.h>

/*  Ввести целое число и определить, верно ли, что в нём ровно одна цифра «9». */

int main(void)
{
    int a, k=0,b=0;
    scanf("%d", &a);
    while(a!=0)
    {
        if ((a%10)!=9)
        {
            k = 0;
            a/=10;
        }
        else if ((a%10)==9)
        {
            b += 1;
            a/=10;
        }
    }
        if (b==1)
        {
            printf("YES");
        }
        else
        {
            printf("NO");
        }

    return 0;

}

задача 9 ##########################################################################################################
#include <stdio.h>

/* Ввести целое число и определить, верно ли, что все его цифры четные. */

int main(void)
{
    int a, k=3;
    scanf("%d", &a);
    while(a!=0)
    {
        if ((a%10)%2==0)
        {
            k = 0;
            a/=10;
        }
        else if ((a%10)%2==1)
        {
            k = 1;
            break;
        }
    }
    if (k==0)
    {
        printf("YES");
    }
    else if (k==1)
    {
        printf("NO");
    }

    return 0;
}


задача 10 ##########################################################################################################
#include <stdio.h>

/* Ввести целое число и определить, верно ли, что все его цифры расположены
 СТРОГО в порядке возрастания. */

int main(void)
{
    int a, k=1;

    scanf("%d", &a);

    while(a!=0)
    {
        int c=0,x=0;
        c = a%10;

      //  printf("poslednee chislo %d\n", c);

        x = a/10%10;

     //   printf("pred poslednee chislo %d\n", x);

        if (c>x)
        {
        //    printf("sravnenie %d > %d\n", c, x);

            k = 0;
            a/=10;
        }



        else
        {
            k = 1;

         //   printf("break \n");

            break;
        }
    }
    if (k==0 || a==0)
    {
        printf("YES");
    }
    else
    {
        printf("NO");
    }

    return 0;
}

задача 11 ##########################################################################################################
#include <stdio.h>

/* Ввести целое число и «перевернуть» его, так чтобы первая цифра стала последней и т.д. */

int main()
{

    int a, b=0;
    scanf("%d", &a);
    while (a!=0)
    {
        b = b*10 + a%10;
        a/=10;
    }
    printf("%d\n", b);

    return 0;
}

задача 13 ##########################################################################################################
#include <stdio.h>

/* Посчитать количество четных и нечетных цифр числа. */

int main()
{
    int a, k=0, x=0;
    scanf("%d", &a);
    while  (a!=0) {

       if ((a%10)%2==0)
        {
            k += 1;
            a/=10;
        }
        else if ((a%10)%2==1)
        {
            x += 1;
             a/=10;
        }
    }
        printf("%d %d", k, x);

    return 0;
}


задача 16 ##########################################################################################################

#include <stdio.h>

/* Составить программу для вычисления НОД с помощью алгоритма Евклида.
Даны два натуральных числа. Найти наибольший общий делитель.
*/

int main()
{
    int a, b=0, t=0;

    scanf("%d%d", &a, &b);

    while (b != 0)
    {
        t = b;
        b = a %b;
        a = t;
    }

    printf("%d", a);

    return 0;
}

задача 19 ##########################################################################################################
#include <stdio.h>

// Ввести натуральное число и определить, верно ли, что сумма его цифр равна 10.

int main()
{
    int a, sum=0;
    scanf("%d", &a);
    while (a != 0)
    {
        sum += a%10;
        a /=10;
    }
    if (sum==10)
    {
        printf("YES");
    }
    else
    {
        printf("NO");
    }

    return 0;
}


задача 20 ##########################################################################################################
#include <stdio.h>

//Проверить число на простоту.

int main (void)
{
    int n,i=0,k=0;
    scanf("%d",&n);
    for (i=2; i<n; i++)
    {
        k=n%i;
        if (k==0) break;
    }
    if (n==0 || n==1)
        printf("NO");
    else if (i > n/2)
        printf("YES");
    else
        printf("NO");
}



