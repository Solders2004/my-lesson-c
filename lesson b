Урок В
задача 1

#include <stdio.h>
int main()
{
    int a=1, b;
    scanf("%d", &b);
    while (a <= b) {
    printf("%d ", a);
    printf("%d ", a*a);
    printf("%d \n", a*a*a);
    a++;
    }
     return 0;
}


задача 2
#include <stdio.h>

int main()
{
    int a, b;
    scanf("%d%d", &a, &b);
    while (a <= b) {
    printf("%d  ", a*a);
    a++;
    }
     return 0;
}

